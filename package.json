    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "dayjs": "^1.9.3",
    "moment": "^2.29.1",
    "next": "^10.0.4",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
